{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"semi": true,
	"printWidth": 100,
	"plugins": [
		"prettier-plugin-svelte",
		"prettier-plugin-tailwindcss",
		"@ianvs/prettier-plugin-sort-imports"
	],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	],
	"importOrder": [
		"<BUILTIN_MODULES>",
		"",
		"<THIRD_PARTY_MODULES>",
		"",
		"^lucide-svelte/(.*)$",
		"",
		"^svelte$",
		"^svelte/(.*)$",
		"",
		"^\\$env(.*)$",
		"^\\$app/(.*)$",
		"",
		"^\\$lib/(?!components)(.*)$",
		"",
		"^\\$lib/components/(.*)$",
		"",
		"^[./]"
	],
	"importOrderParserPlugins": [
		"typescript",
		"jsx",
		"decorators-legacy"
	],
	"importOrderTypeScriptVersion": "5.0.0",
	"importOrderCaseSensitive": false
}